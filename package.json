{
  "name": "grafana-javascript-agent",
  "version": "0.0.0",
  "description": "Monorepo for Grafana JavaScript Agent",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/grafana-javascript-agent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/grafana-javascript-agent.git"
  },
  "private": true,
  "workspaces": [
    "packages/**",
    "demo"
  ],
  "main": "index.js",
  "scripts": {
    "start": "lerna run --parallel start",
    "build": "lerna run build",
    "clean": "run-s clean:*",
    "clean:packages": "lerna run --parallel clean",
    "clean:root": "rimraf .build-cache .cache .parcel-cache",
    "quality": "lerna run --parallel quality",
    "quality:test": "lerna run --parallel quality:test",
    "quality:format": "lerna run --parallel quality:format",
    "quality:lint": "run-p quality:lint:*",
    "quality:lint:packages": "lerna run --parallel quality:lint",
    "quality:lint:root": "markdownlint README.md",
    "quality:circular-deps": "lerna run --parallel quality:circular-deps",
    "publish": "lerna publish",
    "git-hooks": "run-s git-hooks:*",
    "git-hooks:pre-commit": "lint-staged"
  },
  "devDependencies": {
    "@grafana/eslint-config": "^2.5.2",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.3",
    "madge": "^5.0.1",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
