{
  "name": "@grafana/agent-tracing-web",
  "version": "0.4.0",
  "description": "Grafana JavaScript Agent web tracing implementation.",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/grafana-javascript-agent",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/grafana-javascript-agent.git",
    "directory": "packages/tracing-web"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "yarn build -w",
    "build": "tsc --build tsconfig.all.json",
    "clean": "rimraf dist/ yarn-error.log",
    "quality": "run-s quality:*",
    "quality:test": "exit 0",
    "quality:format": "prettier --ignore-path ../../.prettierignore --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint": "run-s quality:lint:*",
    "quality:lint:eslint": "eslint \"src/**/*.ts\"",
    "quality:lint:prettier": "prettier --ignore-path ../../.prettierignore --check **/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}",
    "quality:lint:md": "markdownlint README.md",
    "quality:circular-deps": "madge --extensions js,jsx,ts,tsx --circular --ts-config=tsconfig.json ."
  },
  "dependencies": {
    "@grafana/agent-core": "^0.4.0",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/context-zone": "^1.3.0",
    "@opentelemetry/core": "^1.3.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.30.0",
    "@opentelemetry/instrumentation": "^0.30.0",
    "@opentelemetry/instrumentation-document-load": "^0.29.0",
    "@opentelemetry/instrumentation-fetch": "^0.30.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.30.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.30.0",
    "@opentelemetry/otlp-transformer": "^0.30.0",
    "@opentelemetry/resources": "^1.3.0",
    "@opentelemetry/sdk-trace-base": "^1.3.0",
    "@opentelemetry/sdk-trace-web": "^1.3.0",
    "@opentelemetry/semantic-conventions": "^1.3.0"
  },
  "resolutions": {
    "@opentelemetry/api-metrics": "^0.29.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "418b7f9ae1d5e6397404d4d6a1938ee557b8575c"
}
