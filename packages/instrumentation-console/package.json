{
  "name": "@grafana/javascript-agent-instrumentation-console",
  "version": "0.0.0",
  "description": "Grafana JavaScript Agent package for both, browsers and Node.js which provides an automatic mechanism for collecting console events.",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/grafana-javascript-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/grafana-javascript-agent.git",
    "directory": "packages/instrumentation-console"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yarn build -w",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist/",
    "quality": "run-s quality:*",
    "quality:test": "exit 0",
    "quality:format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint": "run-s quality:lint:*",
    "quality:lint:ts": "eslint \"src/**/*.ts\"",
    "quality:lint:md": "markdownlint README.md",
    "quality:circular-deps": "madge --extensions js,jsx,ts,tsx --circular --ts-config=tsconfig.json ."
  },
  "dependencies": {
    "@grafana/javascript-agent-core": "^0.0.0"
  }
}
