{
  "name": "@grafana/faro-demo",
  "version": "1.0.0-beta5",
  "description": "Demo of Faro",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/faro-web-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/faro-web-sdk.git",
    "directory": "demo"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn serve:dev",
    "serve:dev": "nodemon src/server/index.ts",
    "serve:prod": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node --experimental-loader ./dist/server/otel.js dist/server/index.js",
    "build": "run-p build:*",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/server/index.ts --outDir dist/server",
    "clean": "rimraf dist/ logs/ yarn-error.log",
    "quality": "run-s quality:*",
    "quality:test": "exit 0",
    "quality:format": "prettier --cache --cache-location=../.cache/prettier/demo --ignore-path ../.prettierignore -w \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint": "run-s quality:lint:*",
    "quality:lint:eslint": "eslint --cache --cache-location ../.cache/eslint/demo --ignore-path ../.eslintignore \"./**/*.{js,jsx,ts,tsx}\"",
    "quality:lint:prettier": "prettier --cache --cache-location=../.cache/prettier/demo --ignore-path ../.prettierignore -c \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint:md": "markdownlint README.md docs/**/*.md",
    "quality:circular-deps": "madge --circular ."
  },
  "dependencies": {
    "@grafana/faro-core": "^1.0.0-beta4",
    "@grafana/faro-react": "^1.0.0-beta5",
    "@grafana/faro-web-sdk": "^1.0.0-beta4",
    "@grafana/faro-web-tracing": "^1.0.0-beta5",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/core": "^1.8.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.34.0",
    "@opentelemetry/instrumentation": "^0.34.0",
    "@opentelemetry/instrumentation-express": "^0.32.0",
    "@opentelemetry/instrumentation-http": "^0.34.0",
    "@opentelemetry/instrumentation-pg": "^0.34.0",
    "@opentelemetry/instrumentation-winston": "^0.31.0",
    "@opentelemetry/resources": "^1.8.0",
    "@opentelemetry/sdk-node": "^0.34.0",
    "@opentelemetry/sdk-trace-base": "^1.8.0",
    "@opentelemetry/sdk-trace-node": "^1.8.0",
    "@opentelemetry/semantic-conventions": "^1.8.0",
    "@reduxjs/toolkit": "^1.9.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "bootstrap": "^5.2.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "prom-client": "^14.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^8.0.0-alpha.4",
    "react-redux": "^8.0.5",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.6.1",
    "redux": "^4.2.0",
    "sass": "^1.57.1",
    "sequelize": "^6.28.0",
    "serve-static": "^1.15.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/serve-static": "^1.15.0",
    "@vitejs/plugin-react": "^3.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "vite": "^4.0.4"
  }
}
