{
  "name": "@grafana/faro-demo",
  "version": "2.0.0-beta-2",
  "description": "Demo of Faro",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/faro-web-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/faro-web-sdk.git",
    "directory": "demo"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn serve:prod",
    "dev": "yarn serve:dev",
    "serve:dev": "nodemon src/server/index.ts",
    "serve:prod": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node --experimental-loader ./dist/server/otel.js dist/server/index.js",
    "build": "run-p 'build:*'",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/server/index.ts --outDir dist/server",
    "clean": "run-s 'clean:*'",
    "clean:remove": "rimraf dist/ logs/ yarn-error.log",
    "clean:recreate": "run-s 'clean:recreate:*'",
    "clean:recreate:folders": "mkdirp logs",
    "clean:recreate:files": "nodetouch logs/.gitkeep",
    "quality": "run-s 'quality:*'",
    "quality:test": "exit 0",
    "quality:format": "prettier --cache --cache-location=../.cache/prettier/demo --ignore-path ../.prettierignore -w \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint": "run-s 'quality:lint:*'",
    "quality:lint:eslint": "eslint --cache --cache-location ../.cache/eslint/demo \"./**/*.{js,jsx,ts,tsx}\"",
    "quality:lint:prettier": "prettier --cache --cache-location=../.cache/prettier/demo --ignore-path ../.prettierignore -c \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint:md": "markdownlint README.md docs/**/*.md",
    "quality:circular-deps": "madge --circular ."
  },
  "dependencies": {
    "@grafana/faro-core": "^2.0.0-beta-2",
    "@grafana/faro-react": "^2.0.0-beta-2",
    "@grafana/faro-web-sdk": "^2.0.0-beta-2",
    "@grafana/faro-web-tracing": "^2.0.0-beta-2",
    "@grpc/grpc-js": "^1.9.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.206.0",
    "@opentelemetry/instrumentation": "^0.206.0",
    "@opentelemetry/instrumentation-express": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/instrumentation-pg": "^0.58.0",
    "@opentelemetry/instrumentation-winston": "^0.50.0",
    "@opentelemetry/resources": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@reduxjs/toolkit": "^2.3.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.2.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.0.0",
    "protobufjs": "^7.2.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^8.0.0-alpha.4",
    "react-redux": "^9.2.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.23.0",
    "redux": "^5.0.1",
    "sass": "^1.57.1",
    "sequelize": "^6.32.1",
    "serve-static": "^2.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-bootstrap": "^0.26.6",
    "@types/serve-static": "^1.15.0",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.0.0",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.0.1",
    "touch": "^3.1.0",
    "vite": "^7.0.0"
  }
}
