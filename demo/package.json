{
  "name": "@grafana/agent-demo",
  "version": "0.4.0",
  "description": "Demo of Grafana JavaScript Agent.",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/grafana-javascript-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/grafana-javascript-agent.git",
    "directory": "demo"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn serve:dev",
    "serve:dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/server/index.ts",
    "serve:prod": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/server/index.js",
    "build": "run-p build:*",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/server/index.ts --outDir dist/server",
    "clean": "rimraf dist/ logs/ yarn-error.log",
    "quality": "run-s quality:*",
    "quality:test": "exit 0",
    "quality:format": "prettier --ignore-path ../.prettierignore -w \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint": "run-s quality:lint:*",
    "quality:lint:eslint": "eslint --ignore-path ../.eslintignore \"./**/*.{js,jsx,ts,tsx}\"",
    "quality:lint:prettier": "prettier --ignore-path ../.prettierignore -c \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint:md": "markdownlint README.md",
    "quality:circular-deps": "madge --circular ."
  },
  "dependencies": {
    "@grafana/agent-core": "~0.4.0",
    "@grafana/agent-integration-react": "~0.4.0",
    "@grafana/agent-tracing-web": "~0.4.0",
    "@grafana/agent-web": "~0.4.0",
    "@opentelemetry/api": "^1.2.0",
    "@opentelemetry/core": "^1.7.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.33.0",
    "@opentelemetry/instrumentation": "^0.33.0",
    "@opentelemetry/instrumentation-express": "^0.31.1",
    "@opentelemetry/instrumentation-http": "^0.33.0",
    "@opentelemetry/instrumentation-pg": "^0.31.1",
    "@opentelemetry/instrumentation-winston": "^0.30.0",
    "@opentelemetry/resources": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.33.0",
    "@opentelemetry/sdk-trace-base": "^1.7.0",
    "@opentelemetry/sdk-trace-node": "^1.7.0",
    "@opentelemetry/semantic-conventions": "^1.7.0",
    "@reduxjs/toolkit": "^1.8.5",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "prom-client": "^14.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^8.0.0-alpha.4",
    "react-redux": "^8.0.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.4.0",
    "redux": "^4.2.0",
    "sass": "^1.54.9",
    "sequelize": "^6.23.0",
    "serve-static": "^1.15.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-redux": "^7.1.24",
    "@types/serve-static": "^1.15.0",
    "@vitejs/plugin-react": "^2.1.0",
    "cross-env": "^7.0.3",
    "vite": "^3.1.3",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
