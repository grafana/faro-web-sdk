{
  "name": "@grafana/frontend-agent-demo",
  "version": "0.0.0",
  "description": "Demo of Grafana Frontend Agent.",
  "license": "Apache-2.0",
  "author": "Grafana Labs",
  "homepage": "https://github.com/grafana/grafana-frontend-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/grafana-frontend-agent.git",
    "directory": "demo"
  },
  "source": "src/index.html",
  "scripts": {
    "start": "parcel serve",
    "build": "parcel build",
    "clean": "rimraf dist/",
    "quality": "run-s quality:*",
    "quality:test": "exit 0",
    "quality:format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,yaml,yml,json}\"",
    "quality:lint": "eslint \"src/**/*.ts\"",
    "quality:circular-deps": "madge --extensions js,jsx,ts,tsx --circular --ts-config=tsconfig.json "
  },
  "dependencies": {
    "@grafana/frontend-agent-core": "^0.0.0",
    "@grafana/frontend-agent-plugin-browser-meta": "^0.0.0",
    "@grafana/frontend-agent-plugin-console": "^0.0.0",
    "@grafana/frontend-agent-plugin-errors": "^0.0.0",
    "@grafana/frontend-agent-plugin-page-meta": "^0.0.0",
    "@grafana/frontend-agent-plugin-performance": "^0.0.0",
    "@grafana/frontend-agent-plugin-tracing": "^0.0.0"
  }
}
